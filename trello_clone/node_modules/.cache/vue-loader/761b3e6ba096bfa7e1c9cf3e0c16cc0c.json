CardAddButton.vue","sourceRoot":"","sourcesContent":["<template>\n  <div v-if=\"adding\" @click=\"toggleCardForm\" class=\"button add\">\n    <svg\n      xmlns=\"http://www.w3.org/2000/svg\"\n      fill=\"none\"\n      viewBox=\"0 0 24 24\"\n      stroke=\"currentColor\"\n    >\n      <path\n        stroke-linecap=\"round\"\n        stroke-linejoin=\"round\"\n        stroke-width=\"2\"\n        d=\"M12 4v16m8-8H4\"\n      />\n    </svg>\n    <p>Add a card</p>\n  </div>\n  <div v-else>\n    <CardAdd :listId=\"listId\" />\n    <div class=\"confirmations\">\n      <input\n        class=\"button confirm\"\n        type=\"submit\"\n        value=\"Add card\"\n        @click=\"toggleCardForm\"\n      />\n      <svg\n        @click=\"toggleCardForm\"\n        xmlns=\"http://www.w3.org/2000/svg\"\n        class=\"button cancel\"\n        fill=\"none\"\n        viewBox=\"0 0 24 24\"\n        stroke=\"currentColor\"\n      >\n        <path\n          stroke-linecap=\"round\"\n          stroke-linejoin=\"round\"\n          stroke-width=\"2\"\n          d=\"M6 18L18 6M6 6l12 12\"\n        />\n      </svg>\n    </div>\n  </div>\n</template>\n\n<script>\nimport CardAdd from \"./CardAdd.vue\";\n\nexport default {\n  name: \"CardAddButton\",\n  data() {\n    return {\n      adding: true,\n    };\n  },\n  props: {\n    listId: Number,\n  },\n  components: {\n    CardAdd,\n  },\n  methods: {\n    toggleCardForm() {\n      this.adding = !this.adding;\n      this.$store.state.currentList = this.listId;\n      //  console.log('test ' + this.$store.state.currentList)\n    },\n  },\n};\n</script>\n<style scoped>\n.button {\n  text-align: start;\n  margin: 0 0 10px 10px;\n  padding: 5px;\n  color: white;\n  border-radius: 3px;\n  border: 0;\n  font-size: 0.9rem;\n}\nsvg {\n  height: 18px;\n  padding: 0 5px;\n}\n.confirmations {\n  display: flex;\n}\n.cancel {\n  text-align: center;\n  color: grey;\n  height: 24px;\n  margin-left: 4px;\n}\n.cancel:hover {\n  color: #333;\n  cursor: pointer;\n}\n.add {\n  display: flex;\n  align-items: center;\n  background-color: none;\n  color: grey;\n  height: 22px;\n  width: 89%;\n}\n.add:hover {\n  background-color: rgb(223, 223, 223);\n  cursor: pointer;\n  color: #333;\n}\n.confirm {\n  text-align: center;\n  background-color: #1980c5;\n  padding: 8px 10px;\n}\n.confirm:hover {\n  background-color: #136eac;\n  cursor: pointer;\n}\n</style>\n"]}]}