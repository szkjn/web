\n        console.log(err.message);\n      });\n  },\n};\n</script>\n\n<style scoped>\n.container {\n  background: #eee;\n  border-radius: 3px;\n  max-width: 300px;\n  min-height: 100px;\n  padding: 10px 0;\n  padding-bottom: 10px;\n}\nh3 {\n  margin: 0;\n  padding: 0 20px;\n  font-family: sans-serif;\n  font-size: 0.95rem;\n  text-align: left;\n}\n.button {\n  padding: 5px;\n  color: white;\n  width: auto;\n}\n.button:hover {\n  cursor: pointer;\n}\n.delete {\n  background: red;\n}\n.add {\n  background: green;\n}\n</style>"]}]}